{
  "name": "webext-llm",
  "displayName": "WebextLLM",
  "version": "0.2.0",
  "private": true,
  "description": "Browser-native LLMs at your fingertips",
  "author": "Ido Salomon <idosalomonh@gmail.com>",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build --no-hoist",
    "package": "plasmo package"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@plasmohq/messaging": "^0.2.1",
    "@plasmohq/storage": "^1.3.1",
    "@types/chrome": "^0.0.231",
    "@types/webextension-polyfill": "^0.10.0",
    "@vespaiach/axios-fetch-adapter": "github:alexanderatallah/axios-fetch-adapter",
    "async-mutex": "^0.4.0",
    "axios": "^1.3.4",
    "axios-retry": "^3.4.0",
    "fast-deep-equal": "^3.1.3",
    "object-hash": "^3.0.0",
    "plasmo": "0.68.3",
    "puro": "^0.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "use-chrome-storage": "^1.2.1",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@plasmohq/rps": "^1.8.4",
    "@types/node": "18.15.11",
    "@types/object-hash": "^3.0.2",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.22",
    "prettier": "2.8.7",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "window.ai": "workspace:*"
  },
  "manifest": {
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost; object-src 'self';"
    },
    "action": {},
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "https://mail.google.com/*"
        ]
      }
    ],
    "host_permissions": [
      "file://*/*",
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage",
      "offscreen",
      "idle"
    ]
  }
}
